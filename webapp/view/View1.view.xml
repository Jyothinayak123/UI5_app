<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="Ui5_application.Ui5_app.controller.View1" displayBlock="true">
	
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					    <Table xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" noDataText="Drop column list items here and columns in the area above" id="table0" cd:w5g.dt.context="/zfcat_logSet">
					        <items>
					            <ColumnListItem type="Active" id="item0">
					                <cells>
					                    <Text id="text0" text="{EmployeeId}"/>
					                    <Text text="{EmployeeName}" id="text1"/>
					                    <Text text="{EmployeeEmail}" id="text2"/>
					                    <Text text="{EmployeePhoneNo}" id="text4"/></cells>
					            </ColumnListItem>
					        </items>
					        <columns>
					            <Column id="column0">
					                <header>
					                    <Label text="Employee ID" id="label0"/>
					                </header>
					            </Column>
					            <Column id="column1">
					                <header>
					                    <Label text="Employee Name" id="label1"/>
					                </header>
					            </Column>
					            <Column id="column2">
					                <header>
					                    <Label text="Employee Email" id="label2"/>
					                </header>
					            </Column>
					            <Column id="column3">
					                <header>
					                    <Text text="Contact No" id="text3"/>
					                </header>
					            </Column></columns></Table>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
